require "nexmo"

client = Nexmo::Client.new(application_id: APPLICATION_ID, private_key: PRIVATE_KEY)
response = client.calls.create({
  to: [{type: 'phone', number: '442079460000'}],
  from: {type: 'phone', number: '442079460001'},
  answer_url: ['https://example.com/call_sales.json']
})

puts response
